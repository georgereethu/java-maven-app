pipeline{
 agent any
 tools{
 maven 'maven-3.9'
 }
 stages{
  stage("build"){
  steps{
      sh 'mvn package'
     }
  }
   stage("Test"){
  

        steps{
          echo  "Test is successful"
               }
   
}

     stage("Build Image"){
  
       steps{
        echo " Build is susccessfull"
       }
   
}

      stage("Deploying Image"){

        steps{
         echo "Deploying docker image to Docker hub"
      }
      }     

     }

   }
