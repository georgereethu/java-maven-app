pipeline{
 agent any
 stages{
  stage("build"){
  
  steps{
      echo "Build is successfull"
     }
  }
   stage("Test"){
  

        steps{
          echo  "Test is successful"
               }
}

   stage("Deploying Image"){

        steps{
         echo "Deploying docker image to Docker hub"
      }
      }     
      post {
        always{
        
        echo "Pipeline is successfull as always"
        }
        }
        

     }

   }
